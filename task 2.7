#include <stdio.h>
#include <stdlib.h>
#define N 64
int main()
{
  int arr[N][2], i;
	int count = 0;
	char c;
	do{
	scanf("%c",&c);
	for(i = 0; i <= count; i++)
		if(arr[i][0] == (int)c)
		{
			arr[i][1] ++;
			break;
		}
		else if (i == count)
		{
			arr[count][0] = c;
			arr[count][1] = 1;
			count++;
			break;
		}
	}while(c != '\n');
	
	for (i = 0; i < count; i++)
			printf("%c %d \n",arr[i][0], arr[i][1]);
	return 0; 
}
