#include <time.h>
#include <stdio.h>
#include <stdlib.h>
#include <string>
#define N 15

int main()
{
  int i = 0, j = N-1;
	char arr[N], tmp;
	printf("Enter %d symbols ", N);
	scanf("%s", arr);
	//strncat(arr,arrTemp,strlen(arrTemp) - 1);
	//strcmp(arr, &arrTemp[strlen(arrTemp) - 1]);
	while(i < j)
	{
		if(arr[i] >= 'a' && arr[i] <= 'z')
			i++;
		else if(arr[i] >= '0' && arr[i] <= '9')
		{
			tmp = arr[i];
			arr[i] = arr[j];
			arr[j] = tmp;
			j--;
		}
		else
		{
			printf("Error simbol %c\n", arr[i]);
			break;
		}
		printf("\n %s", arr);
	}
	return 0;
}
 
