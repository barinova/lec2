#include <stdlib.h>
#include <stdio.h>

int main()
{
	char c;  
	int countSymb = 0, countSpace = 0;
	do {
        scanf("%c", &c);
		while(c == ' ')
		{
			if(countSymb == 0)
				scanf("%c", &c);
			countSpace ++;
		}
		if(countSpace > 0)
			printf(" %c", c);
		else 
			printf("%c", c);
		countSpace = 0;
	}while(c!='\n');
	return 0 ;
	}
